 !
 ! General
 !urxvt*geometry:         80x26
 urxvt.termName:  rxvt-256color
 urxvt.loginShell:         true
 urxvt.scrollBar:         false
 urxvt.secondaryScroll:    true
 urxvt.saveLines:         65535
 urxvt.cursorBlink:       false
 !urxvt.urgentOnBell:       true
 !urxvt.borderLess:       false
 !urxvt.borderWidth:          1
 !urxvt.internalBorder:       0
 !
 !
 ! Appearance
 !
 urxvt.font: xft:Ubuntu:size=12:antialias=true:hinting=true
 ! Cursor
 urxvt.cursorColor: #DCDCCC
 *foreground: #DDEEDD
 *background: #1C1C1C

 !black
 *color0: #1C1C1C
 *color8: #4d4d4d
 !red
 *color1: #D81860
 *color9: #F00060
 !green
 *color2: #B7CE42
 *color10: #BDE077
 !yellow
 *color3: #FEA63C
 *color11: #FFE863
 !blue
 *color4: #66AABB
 *color12: #AACCBB
 !magenta
 *color5: #9E6FFE
 *color13: #9E6FFE
 !cyan
 *color6: #5E7175
 *color14: #A3BABF
 !white
 *color7: #DDEEDD
 *color15: #6C887A
 !magenta
 *color5: #B7416E
 *color13: #BB4466
